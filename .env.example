# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=your password
DB_NAME=bithumb_data

# 애플리케이션 설정
NODE_ENV=production
LOG_LEVEL=info
LOG_MAX_FILES=30d
LOG_MAX_SIZE=20m

# Bithumb API 설정
BITHUMB_API_URL=https://api.bithumb.com
API_REQUEST_TIMEOUT=10000

# 데이터 수집 설정
DEFAULT_INTERVAL=60000
MAX_RETRIES=3
RETRY_DELAY=5000